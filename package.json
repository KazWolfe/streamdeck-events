{
  "name": "@rweich/streamdeck-events",
  "version": "0.0.0-development",
  "description": "A colletion of streamdeck events in typescript.",
  "author": "rweich",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ci": "yarn type-check && yarn lint && yarn lint-tests && yarn test",
    "clean": "rm -rf dist/*",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "eslint ./src/**/*.ts",
    "lint-tests": "eslint test/**/*.ts",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha",
    "test-coverage": "nyc yarn test",
    "test-coverage-report": "yarn test-coverage && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.16.1",
    "ajv": "^7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rweich/eslint-config": "^1.0.1",
    "@rweich/prettier-config": "^1.0.0",
    "@types/chai": "^4.2.16",
    "@types/chai-json-schema": "^1.4.6",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "chai-json-schema": "^1.5.1",
    "coveralls": "^3.1.0",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "organize-imports-cli": "^0.8.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rweich/streamdeck-events.git"
  },
  "keywords": [
    "streamdeck",
    "elgato",
    "typescript",
    "sdk"
  ],
  "files": [
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@rweich"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "organize-imports-cli",
      "prettier --write"
    ]
  },
  "prettier": "@rweich/prettier-config",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
